<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">

        <em:id>MeihuaCC@shyangs</em:id>
        <em:name>Meihua Chinese Converter</em:name>
        <em:version>2.3.1</em:version>
        <em:iconURL>chrome://meihuacc/skin/icon32.png</em:iconURL>
        <em:optionsURL>chrome://meihuacc/content/options.xul</em:optionsURL>
        <em:type>2</em:type>

        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>22.0</em:minVersion>
                <em:maxVersion>55.*</em:maxVersion>
            </Description>
        </em:targetApplication>

        <em:bootstrap>true</em:bootstrap>
        <em:multiprocessCompatible>false</em:multiprocessCompatible>

        <em:localized>
            <Description>
                <em:locale>en-US</em:locale>
                <em:name>Meihua Chinese Converter</em:name>
                <em:description>This addon converts page between Simplified and Traditional Chinese.</em:description>
                <em:creator>shyangs</em:creator>
                <em:homepageURL>https://github.com/shyangs/Meihua-Chinese-Converter</em:homepageURL>
                <em:developer>shyangs</em:developer>
                <em:translator>Jixun Wu</em:translator>
            </Description>
        </em:localized>

        <em:localized>
            <Description>
                <em:locale>zh-TW</em:locale>
                <em:name>Meihua Chinese Converter</em:name>
                <em:description>梅花繁簡轉換器，可自動或手動執行繁體字、簡體字之間的相互轉換。</em:description>
                <em:creator>shyangs</em:creator>
                <em:homepageURL>https://github.com/shyangs/Meihua-Chinese-Converter</em:homepageURL>
                <em:developer>shyangs</em:developer>
            </Description>
        </em:localized>

        <em:localized>
            <Description>
                <em:locale>zh-CN</em:locale>
                <em:name>Meihua Chinese Converter</em:name>
                <em:description>梅花繁简转换器，可自动或手动执行繁体字、简体字之间的相互转换。</em:description>
                <em:creator>shyangs</em:creator>
                <em:homepageURL>https://github.com/shyangs/Meihua-Chinese-Converter</em:homepageURL>
                <em:developer>shyangs</em:developer>
				<em:translator>文科</em:translator>
            </Description>
        </em:localized>

    </Description>
</RDF>
